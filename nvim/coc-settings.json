{
  "suggest.enablePreselect": false,
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "explorer.diagnostic.displayMax": 0,
  "explorer.git.enable": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.icon.source": "nvim-web-devicons",
  "explorer.openAction.strategy": "sourceWindow",
  "explorer.sources":[
    {
      "name": "file",
      "expand": true
    }
  ],
  "explorer.file.root.template": "[icon] [root]",
  "translator.engines": [
      "bing",
      "youdao",
      "haici"
  ],
  "languageserver": {},
  "go.goplsOptions": {
    "allowModfileModifications": true,
    "experimentalWorkspaceModule": true,
    // "gofumpt": true,
    "staticcheck": true
  },
  "python.venvPath": "/work/python_venv/",
  "yaml.format.enable": true,
  "list.insertMappings": {
    "<C-f>": "feedkeys:\\<C-f>",
    "<C-b>": "feedkeys:\\<C-b>",
    "<C-n>": "normal:j",
    "<C-p>": "normal:k",
    "<C-u>": "feedkeys:\\<C-u>"
  }
}
