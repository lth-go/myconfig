{
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.errorSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.warningSign": "",
  "languageserver": {},
  "list.nextKeymap": "<C-n>",
  "list.previousKeymap": "<C-p>",
  "outline.autoPreview": true,
  "outline.sortBy": "category",
  "explorer.diagnostic.displayMax": 0,
  "explorer.git.enable": false,
  "explorer.file.root.template": "[icon] [root]",
  "explorer.file.child.template": "[selection | clip | 1] [indent][icon | 1] [diagnosticError & 1][filename omitCenter 1][modified][readonly] [linkIcon & 1][link growRight 1 omitCenter 5]",
  "explorer.filename.colored.enable": false,
  "explorer.icon.enableNerdfont": true,
  "explorer.icon.source": "nvim-web-devicons",
  "explorer.openAction.strategy": "previousWindow",
  "explorer.sources": [
    {
      "name": "file",
      "expand": true
    }
  ],
  "go.goplsOptions": {
    "memoryMode": "DegradeClosed",
    // "gofumpt": true,
    "staticcheck": true,
    "hints": {
      "assignVariableTypes": true,
      "constantValues": true
    }
  },
  "python.venvPath": "/work/python_venv/",
  "rust-analyzer.updates.checkOnStartup": false,
  "sql.formatOptions": {
    "language": "postgresql",
    "uppercase": true
  },
  "sumneko-lua.checkUpdate": false,
  "yaml.format.enable": true,
}
